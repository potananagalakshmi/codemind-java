import java.util.Scanner;
class sum{
     public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n1 ,n2,t;
        n1=sc.nextInt();
        n2=sc.nextInt();
        t=n1+n2;
        System.out.println("Sum of" + n1 + " and " + n2 +"is" +t);
        System.out.printf("Sum of %d and %d is:%d",n1,n2,t);
        sc.close();

    }
}
